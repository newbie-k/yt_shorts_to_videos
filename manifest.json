{
  "manifest_version": 3,
  "name": "Youtube shorts to videos",
  "description": "Next Level Extension",
  "version": "1.0",
  "declarative_net_request": {
      "rule_resources": [{
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules_1.json"
      }]
  },
  "permissions": [
      "declarativeNetRequest",
      "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
      "https://www.youtube.com/*"
  ]
}
